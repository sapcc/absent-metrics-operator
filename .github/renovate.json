{
  "extends": [
    "config:base",
    "default:pinDigestsDisabled",
    "docker:enableMajor",
    "regexManagers:dockerfileVersions"
  ],
  "assignees": [
    "talal"
  ],
  "constraints": {
    "go": "1.17"
  },
  "postUpdateOptions": [
    "gomodTidy1.17",
    "gomodUpdateImportPaths"
  ],
  "packageRules": [
    {
      "enabled": false,
      "matchDepTypes": [
        "action"
      ]
    },
    {
      "enabled": false,
      "matchUpdateTypes": [
        "major"
      ],
      "matchPackagePrefixes": [
        "k8s.io/client-go"
      ]
    }
  ],
  "prHourlyLimit": 0
}
